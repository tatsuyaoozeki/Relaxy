<%= form_with(model: @staff, local: true, url: new_user_staff_path) do |form| %>
  

  <div class="form-group">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>

  <div class="field">
    <label><%= form.radio_button :gender, "男" %> 男</label>
    <label><%= form.radio_button :gender, "女" %> 女</label>
  </div>

  <div class="form-group">
    <%= form.submit "登録", class: 'btn btn-primary' %>
  </div>
<% end %>
